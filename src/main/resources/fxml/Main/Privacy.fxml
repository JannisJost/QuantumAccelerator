                              <Button contentDisplay="TEXT_ONLY" minHeight="0.0" minWidth="0.0" mnemonicParsing="false" onMouseClicked="#showToolTipDNSCache" prefHeight="20.0" prefWidth="20.0" styleClass="btnHelp" text="?">
                                 <font>
                                    <Font size="10.0" />
                                 </font>
                                 <cursor>
                                    <Cursor fx:constant="HAND" />
                                 </cursor>
                              </Button>
                           </graphic>
                        </CheckBox>
                        <CheckBox fx:id="chkDeleteCookies" mnemonicParsing="false" text="Delete browser cookies" />
                        <CheckBox fx:id="chkDeleteBrowserHistory" mnemonicParsing="false" text="Delete browser history" />
                     </children>
                     <padding>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </padding>
                  </VBox>
                  <ButtonBar prefHeight="40.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
                    <buttons>
                      <Button fx:id="btnRun" mnemonicParsing="false" onAction="#applyPrivacyOptions" text="Run" />
                    </buttons>
                     <padding>
                        <Insets right="5.0" />
                     </padding>
                  </ButtonBar>
               </children>
            </AnchorPane>
          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="160.0" prefWidth="100.0">
               <children>
                  <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0" spacing="10.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                     <padding>
                        <Insets top="5.0" />
                     </padding>
                     <children>
                        <GridPane hgap="5.0" vgap="10.0">
                          <columnConstraints>
                            <ColumnConstraints hgrow="SOMETIMES" maxWidth="295.0" minWidth="10.0" prefWidth="260.0" />
                            <ColumnConstraints hgrow="SOMETIMES" maxWidth="228.0" minWidth="0.0" prefWidth="44.0" />
                          </columnConstraints>
                          <rowConstraints>
                            <RowConstraints minHeight="10.0" prefHeight="50.0" vgrow="SOMETIMES" />
                            <RowConstraints minHeight="10.0" prefHeight="50.0" vgrow="SOMETIMES" />
                            <RowConstraints minHeight="10.0" prefHeight="50.0" vgrow="SOMETIMES" />
                              <RowConstraints minHeight="10.0" prefHeight="50.0" vgrow="SOMETIMES" />
                          </rowConstraints>
                           <children>
                              <Button fx:id="btnTelemetryOptions" mnemonicParsing="false" onAction="#showTelemetryOptions" prefHeight="50.0" prefWidth="285.0" text="Telemetry options" />
                              <Button fx:id="btnPasswordGenerator" mnemonicParsing="false" onAction="#showPasswordGenerator" prefHeight="50.0" prefWidth="285.0" text="Password generator" GridPane.rowIndex="1" />
                              <Button fx:id="btnDeactivateCam" contentDisplay="RIGHT" mnemonicParsing="false" onAction="#activateOrDeactivateCam" prefHeight="50.0" prefWidth="285.0" text="Deactivate/activate cam" GridPane.rowIndex="2">
                                 <graphic>
                                    <ProgressIndicator fx:id="progChangingCamState" prefHeight="30.0" visible="false" />
                                 </graphic>
                              </Button>
                              <Button fx:id="btnFileShredder" mnemonicParsing="false" prefHeight="50.0" prefWidth="285.0" text="File shredder" GridPane.rowIndex="3" />
                              <Button fx:id="btnHelpTelemetry" contentDisplay="TEXT_ONLY" minHeight="0.0" minWidth="0.0" mnemonicParsing="false" onMouseClicked="#showToolTipTelemetry" prefHeight="20.0" prefWidth="20.0" styleClass="btnHelp" text="?" GridPane.columnIndex="1">
                                 <font>
                                    <Font size="10.0" />
                                 </font>
                                 <cursor>
                                    <Cursor fx:constant="HAND" />
                                 </cursor>
                              </Button>
                              <Button fx:id="btnHelpPwGen" contentDisplay="TEXT_ONLY" layoutX="268.0" layoutY="25.0" minHeight="0.0" minWidth="0.0" mnemonicParsing="false" onMouseClicked="#showToolTipPwGen" prefHeight="20.0" prefWidth="20.0" styleClass="btnHelp" text="?" GridPane.columnIndex="1" GridPane.rowIndex="1">
                                 <font>
                                    <Font size="10.0" />
                                 </font>
                                 <cursor>
                                    <Cursor fx:constant="HAND" />
                                 </cursor>
                              </Button>
                              <Button fx:id="btnHelpCam" contentDisplay="TEXT_ONLY" layoutX="268.0" layoutY="85.0" minHeight="0.0" minWidth="0.0" mnemonicParsing="false" onMouseClicked="#showToolTipCam" prefHeight="20.0" prefWidth="20.0" styleClass="btnHelp" text="?" GridPane.columnIndex="1" GridPane.rowIndex="2">
                                 <font>
                                    <Font size="10.0" />
                                 </font>
                                 <cursor>
                                    <Cursor fx:constant="HAND" />
                                 </cursor>
                              </Button>
                           </children>
                           <VBox.margin>
                              <Insets left="5.0" />
                           </VBox.margin>
                        </GridPane>
                     </children></VBox>
               </children>
            </AnchorPane>
        </items>
      </SplitPane>
   </children>
