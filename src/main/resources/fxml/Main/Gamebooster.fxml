<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.main.GameboosterController">
   <children>
      <SplitPane dividerPositions="0.5" layoutX="200.0" layoutY="14.0" prefHeight="160.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <items>
          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="160.0" prefWidth="100.0">
               <children>
                  <SplitPane dividerPositions="0.5" orientation="VERTICAL" prefHeight="200.0" prefWidth="160.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                    <items>
                      <AnchorPane fx:id="anchorPaneGameboosterSwitch" minHeight="0.0" minWidth="0.0" prefHeight="100.0" prefWidth="160.0">
                           <children>
                              <VBox fx:id="vboxGameboosterSwitch" alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0" spacing="5.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                 <children>
                                    <Label fx:id="lblGameboostStatus" alignment="CENTER" prefHeight="82.0" prefWidth="293.0" text="deactivated" textFill="WHITE">
                                       <font>
                                          <Font name="HoloLens MDL2 Assets" size="45.0" />
                                       </font>
                                    </Label>
                                    <ToggleButton fx:id="toggleBtnGameboost" mnemonicParsing="false" onAction="#activateOrDeactivateGameboost" styleClass="button" text="Gameboost" textFill="WHITE" VBox.vgrow="ALWAYS">
                                       <tooltip>
                                          <Tooltip text="Activates or deactivates Gameboost" />
                                       </tooltip>
                                    </ToggleButton>
                                 </children>
                              </VBox>
                           </children>
                        </AnchorPane>
                      <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="100.0" prefWidth="160.0">
                           <children>
                              <VBox prefHeight="200.0" prefWidth="100.0" spacing="10.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                 <padding>
                                    <Insets left="5.0" top="5.0" />
                                 </padding>
                                 <children>
                                    <Label fx:id="lblOnDeactivateBooster" text="On deactivate Gamebooster:">
                                       <tooltip>
                                          <Tooltip text="Select what happens when you turn Gamebooster off" />
                                       </tooltip>
                                    </Label>
                                    <GridPane fx:id="gridSettingsDeactivateGamebooster">
                                      <columnConstraints>
                                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="180.0" minWidth="10.0" prefWidth="180.0" />
                                        <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" maxWidth="139.0" minWidth="10.0" prefWidth="108.0" />
                                      </columnConstraints>
                                      <rowConstraints>
                                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                      </rowConstraints>
                                       <children>
                                          <Label fx:id="lblResetPowerPlan" text="Reset power plan (rec)" />
                                          <Label fx:id="lblResetCPUPrio" text="Reset CPU priority (rec)" GridPane.rowIndex="1" />
                                          <Label fx:id="lblResetSysMain" text="Reset SysMain" GridPane.rowIndex="2" />
                                       </children>
                                    </GridPane>
                                 </children>
                              </VBox>
                           </children>
                        </AnchorPane>
                    </items>
                  </SplitPane>
               </children>
            </AnchorPane>
          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="160.0" prefWidth="100.0">
               <children>
                  <VBox layoutX="48.0" layoutY="140.0" prefHeight="200.0" prefWidth="100.0" spacing="10.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                     <children>
                        <Button fx:id="btnFreeStandbyRAM" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#freeStandbyRAM" prefHeight="50.0" prefWidth="285.0" text="Clean standby RAM" textFill="WHITE" VBox.vgrow="ALWAYS" />
                        <Separator prefWidth="200.0" />
                        <CheckBox fx:id="chkDelInstallersFromDownload" mnemonicParsing="false" selected="true" text="Delete installers from downloads" />
                     </children>
                     <padding>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </padding></VBox>
                  <ButtonBar prefHeight="40.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
                    <buttons>
                      <Button fx:id="btnApplyCheckBoxes" mnemonicParsing="false" onAction="#runSelected" text="Run" textFill="WHITE" />
                    </buttons>
                     <padding>
                        <Insets right="5.0" />
                     </padding>
                  </ButtonBar>
               </children>
            </AnchorPane>
        </items>
      </SplitPane>
   </children>
</AnchorPane>
